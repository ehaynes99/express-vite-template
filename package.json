    "build:ui": "vite build",
    "test": "jest",
    "preview": "vite preview"
  },
  "dependencies": {
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-gallery": "^1.2.11",
    "socket.io": "^4.5.3",
    "socket.io-client": "^4.5.3"
  },
  "devDependencies": {
    "@swc/core": "^1.3.14",
    "@swc/jest": "^0.2.23",
    "@testing-library/react": "^13.4.0",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.2",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint-config-erich": "^1.0.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "sass": "^1.56.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2"
