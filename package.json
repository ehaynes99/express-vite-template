{
  "name": "express-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "prettier": "eslint-config-erich/.prettierrc.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon -x 'ts-node -P tsconfig.server.json' ./src/server/server.ts",
